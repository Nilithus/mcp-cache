{
    "name": "ql/mcp-cache",
    "description": "Caching standard for mcp services",
    "homepage": "http://git/skluck/mcp-cache",
    "license": "proprietary",
    "authors": [
        {
            "name": "Steve Kluck",
            "email": "stevekluck@quickenloans.com"
        }
    ],

    "archive": {
        "exclude": ["/bin", "/phpunit.xml.dist", "/tests"]
    },
    "autoload": {
        "psr-4": {
            "MCP\\Cache\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "conflict": {
        "ql/mcp": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://composer/"
        }
    ],
    "require": {
        "ql/mcp-core": ">=1"
    },
    "require-dev": {
        "mockery/mockery": "*",
        "phpunit/phpunit": "3.7.*",
        "phpunit/php-invoker": "*"
    }
}
